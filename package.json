{
  "name": "floresya-ecommerce",
  "version": "2.0.0",
  "description": "FloresYa E-commerce - TypeScript Enterprise Edition",
  "type": "module",
  "main": "dist/app/server.js",
  "scripts": {
    "build": "tsc --project tsconfig.prod.json",
    "start": "npm run build && node dist/app/server.js",
    "dev": "tsc --watch & nodemon --delay 2 dist/app/server.js",
    "dev:build": "tsc && node dist/app/server.js",
    "ts:watch": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type:check": "tsc --noEmit",
    "clean": "rm -rf dist/",
    "reset": "npm run clean && npm run build"
  },
  "keywords": [
    "ecommerce",
    "flowers",
    "typescript",
    "enterprise",
    "api-rest"
  ],
  "author": "FloresYa Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.1",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
