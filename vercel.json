{
  "version": 2,
  "name": "floresya-ecommerce",
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "dist/app/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/app/server.js"
    },
    {
      "src": "/dist/(.*)",
      "dest": "dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/app/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "dist/app/server.js": {
      "maxDuration": 30
    }
  }
}