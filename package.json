{
  "name": "floresya-ecommerce",
  "version": "2.0.0",
  "description": "ðŸŒ¸ FloresYa - Enterprise E-commerce Platform for Flower Delivery",
  "type": "module",
  "main": "dist/backend/app/server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run build:backend:watch\" \"npm run build:frontend:watch\" \"npm run dev:server\"",
    "dev:server": "nodemon dist/backend/app/server.js",
    "build": "npm run clean && npm run build:css && npm run build:backend && npm run build:frontend",
    "build:backend": "tsc -p tsconfig.json",
    "build:backend:watch": "tsc -p tsconfig.json --watch",
    "build:frontend": "tsc -p src/frontend/tsconfig.frontend.json && node scripts/post-build-frontend.js",
    "build:frontend:watch": "tsc -p src/frontend/tsconfig.frontend.json --watch",
    "build:css": "postcss src/styles.css -o public/css/styles.css",
    "build:css:watch": "postcss src/styles.css -o public/css/styles.css --watch",
    "build:prod": "NODE_ENV=production npm run build",
    "build:robust": "node scripts/robust-build.mjs",
    "clean": "rimraf dist",
    "start": "node dist/backend/app/server.js",
    "start:prod": "NODE_ENV=production node dist/backend/app/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "type:check": "tsc --noEmit",
    "type:check:backend": "tsc -p tsconfig.json --noEmit",
    "type:check:frontend": "tsc -p src/frontend/tsconfig.frontend.json --noEmit",
    "format": "prettier --write \"**/*.{js,ts,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md,css}\"",
    "validate": "npm run lint:check && npm run type:check && npm run test:run",
    "validate:quick": "npm run lint:check && npm run type:check",
    "validate:critical": "node scripts/master.js",
    "migrate:imports": "node scripts/migrate-imports.mjs",
    "migrate:imports:preview": "node scripts/migrate-imports.mjs --dry-run",
    "mime:autofix": "node scripts/mime-autofix-master.js autofix",
    "mime:diagnose": "node scripts/mime-autofix-master.js diagnose",
    "mime:test": "node scripts/test-mime-comprehensive.js",
    "postinstall": "npm run build:css",
    "vercel-build": "npm run build:prod"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "glob": "^11.0.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tailwindcss": "^4.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "ecommerce",
    "flowers",
    "typescript",
    "express",
    "supabase",
    "enterprise",
    "vercel"
  ],
  "author": "FloresYa Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/floresya/ecommerce-platform"
  },
  "bugs": {
    "url": "https://github.com/floresya/ecommerce-platform/issues"
  },
  "homepage": "https://floresya.vercel.app"
}
