{
  "name": "floresya-ecommerce",
  "version": "2.0.0",
  "description": "FloresYa E-commerce - TypeScript Enterprise Edition",
  "type": "module",
  "main": "dist/app/server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "npm run clean && npx tsc --project tsconfig.prod.json",
    "start": "node dist/app/server.js",
    "vercel-build": "npm run build",
    "dev": "npx tsc --watch & nodemon --delay 2 dist/app/server.js",
    "dev:build": "npx tsc && node dist/app/server.js",
    "ts:watch": "npx tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type:check": "npx tsc --noEmit",
    "clean": "rm -rf dist/",
    "reset": "npm run clean && npm run build",
    "schema:update": "npx ts-node /home/manager/Sync/ecommerce-floresya/dist/utils/schema-extractor.js"
  },
  "keywords": [
    "ecommerce",
    "flowers",
    "typescript",
    "enterprise",
    "api-rest"
  ],
  "author": "FloresYa Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vercel/node": "^5.3.22",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
