{
  "name": "floresya-ecommerce",
  "version": "2.0.0",
  "description": "FloresYa E-commerce - TypeScript Enterprise Edition",
  "type": "module",
  "main": "dist/backend/app/server.js",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build:prod",
    "build:dev": "tsc -p tsconfig.node.json && tsc -p src/frontend/tsconfig.frontend.json",
    "build:frontend:static": "mkdir -p dist/frontend && cp -r public/* dist/frontend/",
    "build:prod": "npm run clean && tsc -p tsconfig.prod.json && npm run build:frontend:static",
    "start": "node dist/backend/app/server.js",
    "vercel-build": "npm run build:prod",
    "dev": "concurrently \"tsc -p tsconfig.node.json --watch\" \"nodemon --watch dist/backend dist/backend/app/server.js\"",
    "dev:build": "npm run build:dev && node dist/backend/app/server.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "type:check": "tsc --noEmit --project tsconfig.node.json",
    "clean": "rm -rf dist/",
    "reset": "npm run clean && npm run build:prod",
    "schema:update": "node dist/backend/utils/schema-extractor.js"
  },
  "keywords": [
    "ecommerce",
    "flowers",
    "typescript",
    "enterprise",
    "api-rest"
  ],
  "author": "FloresYa Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "~4.21.2",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "~7.2.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "~1.4.5-lts.1",
    "sharp": "^0.34.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vercel/node": "^5.3.23",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.0.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "npm@10.9.0"
}
