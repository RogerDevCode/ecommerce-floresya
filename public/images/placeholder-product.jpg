<svg width="300" height="300" viewBox="0 0 300 300" fill="none" xmlns="http://www.w3.org/2000/svg">
<rect width="300" height="300" fill="#F5F5F5"/>
<!-- Simple flower icon -->
<g transform="translate(150, 150)">
  <!-- Stem -->
  <rect x="-2" y="20" width="4" height="60" fill="#28A745"/>
  
  <!-- Leaves -->
  <ellipse cx="-15" cy="40" rx="8" ry="15" fill="#28A745" transform="rotate(-30)"/>
  <ellipse cx="15" cy="50" rx="8" ry="15" fill="#28A745" transform="rotate(30)"/>
  
  <!-- Flower petals -->
  <ellipse cx="0" cy="-20" rx="12" ry="25" fill="#FF69B4"/>
  <ellipse cx="17" cy="-10" rx="12" ry="25" fill="#FF69B4" transform="rotate(60)"/>
  <ellipse cx="17" cy="10" rx="12" ry="25" fill="#FF69B4" transform="rotate(120)"/>
  <ellipse cx="0" cy="20" rx="12" ry="25" fill="#FF69B4" transform="rotate(180)"/>
  <ellipse cx="-17" cy="10" rx="12" ry="25" fill="#FF69B4" transform="rotate(240)"/>
  <ellipse cx="-17" cy="-10" rx="12" ry="25" fill="#FF69B4" transform="rotate(300)"/>
  
  <!-- Center -->
  <circle cx="0" cy="0" r="8" fill="#FFD700"/>
</g>

<text x="150" y="260" font-family="Arial, sans-serif" font-size="14" fill="#999" text-anchor="middle">Imagen no disponible</text>
</svg>