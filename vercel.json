{
  "version": 2,
  "name": "floresya-ecommerce",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|webp|svg|ico|woff|woff2|ttf|eot|html))",
      "dest": "/frontend/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/frontend/images/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/frontend/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/frontend/js/$1"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/frontend/pages/$1"
    },
    {
      "src": "/test-frontend.html",
      "dest": "/test-frontend.html"
    },
    {
      "src": "/debug-images.html",
      "dest": "/debug-images.html"
    },
    {
      "src": "/final-test.html",
      "dest": "/final-test.html"
    },
    {
      "src": "/simple-test.html",
      "dest": "/simple-test.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
